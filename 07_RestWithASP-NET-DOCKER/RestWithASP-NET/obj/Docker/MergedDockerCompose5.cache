name: dockercompose5009751567444514012
services:
  db:
    build:
      context: C:\Users\tcxdr\OneDrive\Área de Trabalho\PROG\RestWithASP-NET\RestWithASP-NET\07_RestWithASP-NET-DOCKER\RestWithASP-NET
      dockerfile: db/Dockerfile
    command:
